#!/usr/bin/env groovy

properties([
    pipelineTriggers([
        [$class: "SCMTrigger", scmpoll_spec: "H/5 * * * *"],
    ])
])

node('') {
  stage ('Checkout') {
    dir('TEST1') {
      git(
      poll: true,//
          url: 'https://github.com/rajneeshkapoor/one.git',
          credentialsId: '4d26c952-7656-4a37-b58a-8ba3c9d4c5e3',
          branch: 'main'
      )
    }
  }
    
    stage ('Run Shell Script 1') {
        echo "Script 1 Stmt 1"
    }
}
